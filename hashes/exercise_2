#difference between merge and merge!

family = {  uncles: ["bob", "joe", "steve"],
            sisters: ["jane", "jill", "beth"],
            brothers: ["frank","rob","david"],
            aunts: ["mary","sally","susan"]
          }
family_2= { parents: ["mary","jean-claude","monique"] }

new=family_2.merge!(family)

p new
p family
p family_2

family = {  uncles: ["bob", "joe", "steve"],
            sisters: ["jane", "jill", "beth"],
            brothers: ["frank","rob","david"],
            aunts: ["mary","sally","susan"]
          }
family_2= { parents: ["mary","jean-claude","monique"] }

new=family_2.merge(family)

p new
p family
p family_2